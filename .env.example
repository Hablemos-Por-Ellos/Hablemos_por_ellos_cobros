###############################################
# Supabase
###############################################
SUPABASE_URL="https://YOUR-PROJECT.supabase.co"
SUPABASE_SERVICE_ROLE_KEY="YOUR_SERVICE_ROLE"


###############################################
# Wompi - SANDBOX (pruebas)
# Llaves para usar en entorno de desarrollo local
###############################################

# Llave pública de sandbox (frontend/widget)
# Debe comenzar con: pub_test_
NEXT_PUBLIC_WOMPI_PUBLIC_KEY_SANDBOX="pub_test_YOUR_PUBLIC_KEY_SANDBOX"

# Llave privada de sandbox (solo backend)
# Debe comenzar con: prv_test_
WOMPI_PRIVATE_KEY_SANDBOX="prv_test_YOUR_PRIVATE_KEY_SANDBOX"

# Secreto de integridad de sandbox (solo backend)
# Debe comenzar con: test_integrity_
WOMPI_INTEGRITY_SECRET_SANDBOX="test_integrity_YOUR_INTEGRITY_SECRET_SANDBOX"

# Secreto de eventos (webhooks) de sandbox (solo backend)
# Debe comenzar con: test_events_
WOMPI_EVENTS_SECRET_SANDBOX="test_events_YOUR_EVENTS_SECRET_SANDBOX"


###############################################
# Wompi - PRODUCCIÓN
###############################################

# Llave pública de producción (frontend/widget)
# Debe comenzar con: pub_prod_
NEXT_PUBLIC_WOMPI_PUBLIC_KEY_PROD="pub_prod_YOUR_PUBLIC_KEY_PROD"

# Llave privada de producción (solo backend)
# Debe comenzar con: prv_prod_
WOMPI_PRIVATE_KEY_PROD="prv_prod_YOUR_PRIVATE_KEY_PROD"

# Secreto de integridad de producción (solo backend)
# Debe comenzar con: prod_integrity_
WOMPI_INTEGRITY_SECRET_PROD="prod_integrity_YOUR_INTEGRITY_SECRET_PROD"

# Secreto de eventos (webhooks) de producción (solo backend)
# Debe comenzar con: prod_events_
WOMPI_EVENTS_SECRET_PROD="prod_events_YOUR_EVENTS_SECRET_PROD"


###############################################
# Compatibilidad / Fallbacks
# Puedes usar estas sin sufijo si sólo manejas
# un set de llaves por entorno.
###############################################

# Ejemplo: sólo sandbox en local
# NEXT_PUBLIC_WOMPI_PUBLIC_KEY="pub_test_YOUR_PUBLIC_KEY_SANDBOX"
# WOMPI_PRIVATE_KEY="prv_test_YOUR_PRIVATE_KEY_SANDBOX"
# WOMPI_INTEGRITY_SECRET="test_integrity_YOUR_INTEGRITY_SECRET_SANDBOX"
# WOMPI_EVENTS_SECRET="test_events_YOUR_EVENTS_SECRET_SANDBOX"

# Ejemplo: sólo producción 
# NEXT_PUBLIC_WOMPI_PUBLIC_KEY="pub_prod_YOUR_PUBLIC_KEY_PROD"
# WOMPI_PRIVATE_KEY="prv_prod_YOUR_PRIVATE_KEY_PROD"
# WOMPI_INTEGRITY_SECRET="prod_integrity_YOUR_INTEGRITY_SECRET_PROD"
# WOMPI_EVENTS_SECRET="prod_events_YOUR_EVENTS_SECRET_PROD"